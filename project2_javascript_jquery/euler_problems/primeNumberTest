// Prime number test

var isPrime = false;

var testPrime = function(number) {
  for (i = 2; i < 10000; i++) {
    if (number % i === 0) {
      console.log(number + " is not a prime number.");
      isPrime = false;
      return;
    } else {
      console.log(number + " is a prime number.");
      isPrime = true;
      return;
    }
  }
};

testPrime(10);
console.log("'isPrime' is " + isPrime);
